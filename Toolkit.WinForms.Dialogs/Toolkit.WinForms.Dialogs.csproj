<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(LibraryFrameworksWindows)</TargetFrameworks>
    <UseWindowsForms>true</UseWindowsForms>
    <RootNamespace>$(BaseRootNamespace).WinForms.Dialogs</RootNamespace>
    <AssemblyName>$(BaseRootNamespace).Winforms.Dialogs</AssemblyName>
    <PackageId>$(BasePackageId).WinForms.Dialogs</PackageId>
    <Product>cmd.wtf WinForms Dialog Toolkit</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ookii.Dialogs.WinForms" Version="1.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Toolkit.Core\Toolkit.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
